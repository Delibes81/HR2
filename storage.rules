rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      // Permitir lectura a todo el mundo (necesario para ver las im√°genes en la web)
      allow read: if true;
      // Permitir escritura solo a usuarios autenticados (Admin)
      allow write: if request.auth != null;
    }
  }
}
